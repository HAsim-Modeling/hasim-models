service ICN_MESH
{
    ##
    ## Initialize the routing table, sending marshalled data.
    ##
    server hw (bsv, connection) <- sw (cpp, method)
    {
        method initRoutingTable(in UINT8[8] marshalledData);
        method initLocalPortTypeMap(in UINT8[8] mapEntry, in UINT8[8] done);
    };
};
